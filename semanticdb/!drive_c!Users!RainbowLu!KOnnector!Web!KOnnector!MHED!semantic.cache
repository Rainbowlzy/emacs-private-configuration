                    $('input[for = \"SpriteCAN300MLPrice\"]').attr(\"checked\", true);
                $('input[name=\"SpriteCAN300MLPrice\"]').val(Number(data.SpriteCAN300MLPrice).toFixed(2));

                if (data.CokeMS12515LAvailability)
                    $('input[for = \"CokePET625750MLPrice\"]').attr(\"checked\", true);
                $('input[name=\"CokePET625750MLPrice\"]').val(Number(data.CokeSpritePET625750MLPrice).toFixed(2));


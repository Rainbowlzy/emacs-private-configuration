                //kb
                if (data.PepsiCANAvailability)
                    $('input[for = \"PepsiCANPrice\"]').attr(\"checked\", true);
                $('input[name=\"PepsiCANPrice\"]').val(Number(data.Pepsi7upMirandaCANPrice).toFixed(2));

                if (data.S7UPCANAvailability)
                    $('input[for = \"S7upCANPrice\"]').attr(\"checked\", true);
                $('input[name=\"S7upCANPrice\"]').val(Number(data.S7UPCANPrice).toFixed(2));

                if (data.Pepsi625MLAvailability)
                    $('input[for = \"Pepsi625MLPrice\"]').attr(\"checked\", true);
                $('input[name=\"Pepsi625MLPrice\"]').val(Number(data.Pepsi7upMiranda625MLPrice).toFixed(2));

                if (data.S7UP625MLAvailability)
                    $('input[for = \"S7up625MLPrice\"]').attr(\"checked\", true);
                $('input[name=\"S7up625MLPrice\"]').val(Number(data.S7UPCANPrice625MLPrice).toFixed(2));

                if (data.Pepsi2LAvailability)
                    $('input[for = \"Pepsi2LPrice\"]').attr(\"checked\", true);
                $('input[name=\"Pepsi2LPrice\"]').val(Number(data.Pepsi7upMiranda2LPrice).toFixed(2));


                if (data.S7UP2LAvailability)
                    $('input[for = \"S7up2LPrice\"]').attr(\"checked\", true);
                $('input[name=\"S7up2LPrice\"]').val(Number(data.S7UPCANPrice2LPrice).toFixed(2));
            }

            $(\".checkbox\").each(function (i) {
                var _input = $(this).parent().parent().siblings().find(\"input\"); //.val();
                if (_input.val() != \"\" && _input.val() != 0) {
                    _input.removeClass(\"disabled\"); //.css(\"background\", \"#fff\");
                } else {
                    _input.val('');
                    _input.addClass(\"disabled\"); //.css(\"background\", \"#EAEAEA\");

                    //选中
                    $(this).parent().addClass(\"active\");
                }
            });

            CloseBox();
        });
    }


    function OnSubmit() {
        var num = 0;
        var istrue = true;
        var _inputs = $(\"table>tbody>tr>td>div>input[type='text']\");
        _inputs.each(function (i) {
            var ch = $(\"input:checkbox\").eq(i).is(\":checked\");
            var value = $(this).val();
            if (value == '' && ch) {
                istrue = false;
                num = i;
            }
        });
        _inputs.css(\"border\", \"0px solid red\");
        if (!istrue) {
            _inputs.eq(num).css(\"border\", \"1px solid red\");
            alert(\"请为所选品项填写价格. <br/>Please fill in price for the selected SKU.\");
            $(\"input[type='text']\").eq(num).blur();
            return;
        }

        //        if (!getNextTip() && $(\".cAvailabilitybody\").find(\"input[type='checkbox']:checked\").length == 0)

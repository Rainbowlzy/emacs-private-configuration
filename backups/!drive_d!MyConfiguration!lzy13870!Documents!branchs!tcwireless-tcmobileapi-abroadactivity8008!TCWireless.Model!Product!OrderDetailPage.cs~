using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace TCWireless.Model.Product
{
    public class OrderStatusEntity
    {
        #region OrderStatusEntity

         codeDesc	状态描述	字符串
createTime	状态时间	字符串


        #endregion
    }
    public class OrderDetailPage
    {
        /// <summary>
        /// 倒计时文案	
        /// </summary>
        public string timerLeftDesc { get; set; }

        /// <summary>
        /// 服务器时间	
        /// </summary>
        public string timerStart { get; set; }

        /// <summary>
        /// 订单结束时间	
        /// </summary>
        public string timerEnd { get; set; }

        /// <summary>
        /// 倒计时文案	
        /// </summary>
        public string timerRightDesc { get; set; }

        /// <summary>
        /// 订单跟踪	
        /// </summary>
        public object orderStatusTrack { get; set; }

        /// <summary>
        /// 订单编号	
        /// </summary>
        public string orderNumber { get; set; }

        /// <summary>
        /// 订单金额	
        /// </summary>
        public string orderAmount { get; set; }

        /// <summary>
        /// 产品ID	
        /// </summary>
        public string productID { get; set; }

        /// <summary>
        /// 产品类型	
        /// </summary>
        public string productType { get; set; }

        /// <summary>
        /// 产品跳转url	
        /// </summary>
        public string productUrl { get; set; }

        /// <summary>
        /// 产品名称	
        /// </summary>
        public string productName { get; set; }

        /// <summary>
        /// 是否邮寄	
        /// </summary>
        public string isGetBySelf { get; set; }

        /// <summary>
        /// 出游人数	
        /// </summary>
        public string peopleNumber { get; set; }

        /// <summary>
        /// 出发日期	
        /// </summary>
        public string fetchDate { get; set; }

        /// <summary>
        /// 联系人	
        /// </summary>
        public string peopleName { get; set; }

        /// <summary>
        /// 手机号码	
        /// </summary>
        public string phoneNumber { get; set; }

        /// <summary>
        /// 预定须知	
        /// </summary>
        public List<string> bookingInfo { get; set; }

        /// <summary>
        /// 客服描述名称	
        /// </summary>
        public string orderDesc { get; set; }

        /// <summary>
        /// 客服跳转链接	
        /// </summary>
        public string orderUrl { get; set; }

        /// <summary>
        /// 顶部客服链接	
        /// </summary>
        public string orderServicesUrl { get; set; }

        /// <summary>
        /// 订单状态	
        /// </summary>
        public string orderStatus { get; set; }

        /// <summary>
        /// 订单ID	
        /// </summary>
        public string orderID { get; set; }

        /// <summary>
        /// 左边按钮的文案	
        /// </summary>
        public string leftBtnName { get; set; }

        /// <summary>
        /// 右边按钮的文案	
        /// </summary>
        public string rightBtnName { get; set; }
    }
}
